' >\r\n            <h1>Checkout</h1>\r\n            {this.state.success !== '' ?   <center><div className='alert alert-success fixed-top'>{this.state.success}</div></center> : ''}\r\n            <form onSubmit={this.handlesubmit}>\r\n\r\n                <label htmlFor=\"name\">Your Name:</label>\r\n                <input type=\"text\"  id='name' name='name' placeholder='Your Name' className='form-control' onChange = {this.handleform} required=\"required\" />\r\n                <label htmlFor=\"surname\">Your Surname:</label>\r\n                <input type=\"text\"  id='surname' name='surname' placeholder='Your Surname' className='form-control' onChange = {this.handleform} required=\"required\" />\r\n                <label htmlFor=\"address\">Your Address:</label>\r\n                <input type=\"text\" required='required' id='address' name='address' placeholder='Address where Product is delivered' className='form-control' required=\"required\" onChange = {this.handleform} />\r\n                <label htmlFor=\"tel_num\">Your Telephone Number:</label>\r\n                <input type=\"number\" required='required' className='form-control' id='tel_num' name=\"tel_num\" placeholder='Your telephone number to connect with you' required=\"required\" onChange = {this.handleform} />\r\n                <br/>\r\n                {this.state.disabled ? <input type=\"submit\" disabled  value=\"Submit\" className='btn btn-info'/> : <input type=\"submit\" value=\"Submit\" className='btn btn-info'/>} \r\n                <br/><br/>\r\n            </form>\r\n\r\n        </div>  );\r\n    }\r\n}\r\n \r\nexport default Form_checkout;","\r\nimport ContactPage from './contact'\r\nimport List from './list'\r\nimport cookie from 'react-cookies'\r\nimport 'whatwg-fetch'\r\nimport React, { Component,useState,useEffect } from 'react';\r\nimport ReactDOM from 'react-dom';\r\nimport PostInline from './compenents/post'\r\nimport Contact from './compenents/contact'\r\nimport Navbar from './compenen