:true,\r\n        posts:[],\r\n        cart:0,\r\n        wishlist:0\r\n      }\r\n      componentDidMount()\r\n      {\r\n        this.setState({\r\n          posts:[],\r\n        })\r\n        fetch('/api1/')\r\n        .then((response) => response.json())\r\n        .then(booksList => {\r\n         if(this.state.posts != booksList)\r\n         {\r\n          this.setState({ posts: booksList,loading:false});\r\n         }  \r\n        });\r\n        axios.get('/sdlkewrws/')\r\n        .then(response => {\r\n          this.setState({\r\n            cart:response.data.cart,\r\n            wishlist:response.data.wishlist\r\n          })\r\n        })\r\n      }\r\n      count(){\r\n        axios.get('/sdlkewrws/')\r\n        .then(response => {\r\n        this.setState({\r\n            cart:response.data.cart,\r\n            wishlist:response.data.wishlist\r\n        })\r\n        })\r\n    }\r\n    render() { \r\n        if(this.state.loading){\r\n          return <Loading />\r\n        }\r\n        else{\r\n          return ( \r\n            <div>\r\n                <Navbar state={this.state} /><br/>\r\n                <div className=\"container\">\r\n                    <h3>Product List </h3><br/>\r\n                    <div className='container'><div className='row'>{this.state.posts.map(post => <PostInline key = {post.id} count={this.count} post = {post} /> )}</div></div> \r\n                </div>\r\n            </div>\r\n         );\r\n        }\r\n    }\r\n}\r\n \r\nexport default List;","import React, { Component } from 'react';\r\n\r\nclass Detail1 extends Component {\r\n    sale = () =>{\r\n        if(this.props.pro.sale)\r\n        {\r\n            return <div><strong>Price:</strong>{this.props.pro.price_in_sale} So'm</div>\r\n        }\r\n        else{\r\n            return <div> <strong>Price:</strong> {this.props.pro.price} So'm</div>\r\n        }\r\n    }\r\n    render() { \r\n        return ( \r\n        <main>\r\n            <br/>\r\n            <br